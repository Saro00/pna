{
  "gpu": {
    "use": true,
    "id": 0
  },
  "model": "EIG",
  "dataset": "PCBA",
  "out_dir": "out/molecules_graph_regression/",
  "params": {
    "seed": 41,
    "epochs": 200,
    "batch_size": 128,
    "init_lr": 0.05,
    "lr_reduce_factor": 0.5,
    "lr_schedule_patience": 100,
    "min_lr": 0.009,
    "weight_decay": 0,
    "print_epoch_interval": 1,
    "max_time": 48
  },
  "net_params": {
    "L": 6,
    "hidden_dim": 100,
    "out_dim": 100,
    "residual": true,
    "JK": "last",
    "edge_feat": false,
    "readout": "mean",
    "in_feat_dropout": 0,
    "dropout": 0,
    "graph_norm": true,
    "batch_norm": true,
    "aggregators": "mean eig1-dx eig2-dx",
    "scalers": "identity",
    "towers": 5,
    "divide_input_first": false,
    "divide_input_last": true,
    "gru": false,
    "edge_dim": 0,
    "pretrans_layers" : 1,
    "posttrans_layers" : 1
  }
}